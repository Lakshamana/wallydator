{
  "type": "module",
  "scripts": {
    "postinstall": "husky init",
    "lint": "eslint .",
    "lintstaged": "npm run lint -- --fix",
    "prepare": "husky",
    "prepare:permission": "chmod ug+x .husky/*",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:ci": "npm test -- --coverage",
    "test:staged": "npm test -- --findRelatedTests",
    "dev": "tsx src/mocks/index.ts",
    "debug": "tsx --inspect-brk src/mocks/index.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && ttsc -p tsconfig-build.json",
    "pack": "npm run build && npm pack",
    "start": "node dist/main/index"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.13.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "git-commit-msg-linter": "^5.0.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "module-alias": "^2.2.3",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ttypescript": "^1.5.15",
    "typescript-transform-paths": "^3.5.3"
  },
  "name": "@lakshamana.dev/wallydator",
  "description": "",
  "version": "1.0.0",
